{
  "name": "must-ui",
 "version": "2.1.3",
  "description": "",
  "type": "module",

  "main": "./dist/cjs/index.js",
  "types": "./dist/esm/index.d.ts",

  "sideEffects": false,

  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    },
    "./accordion": {
      "import": "./dist/esm/components/Accordion/index.js",
      "require": "./dist/cjs/components/Accordion/index.js",
      "types": "./dist/esm/components/Accordion/index.d.ts"
    },
    "./button": {
      "import": "./dist/esm/components/Button/index.js",
      "require": "./dist/cjs/components/Button/index.js",
      "types": "./dist/esm/components/Button/index.d.ts"
    },
    "./input": {
      "import": "./dist/esm/components/Input/index.js",
      "require": "./dist/cjs/components/Input/index.js",
      "types": "./dist/esm/components/Input/index.d.ts"
    },
    "./sheet": {
      "import": "./dist/esm/components/Sheet/index.js",
      "require": "./dist/cjs/components/Sheet/index.js",
      "types": "./dist/esm/components/Sheet/index.d.ts"
    },
    "./dialog": {
      "import": "./dist/esm/components/Dialog/index.js",
      "require": "./dist/cjs/components/Dialog/index.js",
      "types": "./dist/esm/components/Dialog/index.d.ts"
    },
       "./textarea": {
      "import": "./dist/esm/components/Textarea/index.js",
      "require": "./dist/cjs/components/Textarea/index.js",
      "types": "./dist/esm/components/Textarea/index.d.ts"
    }
  },

  "scripts": {
    "build": "pnpm build:esm && pnpm build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\""
  },

  "repository": {
    "type": "git",
    "url": "git+https://github.com/ilaydaufelek/MustUI"
  },
  "homepage": "https://must-ui-studio.vercel.app",

  "peerDependencies": {
    "react": ">=18"
  },

  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],

  "keywords": ["react", "typescript"],
  "author": "ilayda Ã¼felek",
  "license": "ISC",

  "devDependencies": {
    "@types/react": "^19.2.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.37.0",
    "eslint-plugin-jsdoc": "^60.8.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  },

  "dependencies": {
    "clsx": "^2.1.1",
    "lucide-react": "^0.555.0"
  }
}
